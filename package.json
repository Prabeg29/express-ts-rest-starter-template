{
  "name": "express-ts-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf lib && npx tsc -p .",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "start:prod": "npm run build && node ./lib/index.js",
    "knex": "node_modules/.bin/knex",
    "make:migration": "yarn knex migrate:make --knexfile src/knexfile.ts",
    "migrate": "yarn knex migrate:latest --knexfile src/knexfile.ts",
    "migrate:rollback": "yarn knex migrate:rollback --knexfile src/knexfile.ts",
    "make:seed": "yarn knex seed:make --knexfile src/knexfile.ts",
    "seed": "yarn knex seed:run --knexfile src/knexfile.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "test": "NODE_ENV=test jest -i --verbose --forceExit --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "knex": "^2.4.2",
    "mysql2": "^3.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.8.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.1",
    "rimraf": "^4.4.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  }
}
